{
  "name": "react-dropdown-aria",
  "version": "1.1.3",
  "description": "Simple and accessible React dropdown component",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:dev\" \"npm run demo:dev\"",
    "build": "npm run lib:prod && npm run demo:prod",
    "lib:dev": "rollup -c -w -o dist/react-dropdown-aria.dev.js",
    "lib:prod": " SET NODE_ENV=production && rollup -c -o dist/react-dropdown-aria.min.js",
    "demo:dev": "webpack-dev-server --mode development --open --hot",
    "demo:prod": "webpack --mode production",
    "deploy:demo": "npm run build && git push origin `git subtree split --prefix demo/dist master`:gh-pages --force",
    "lint": "eslint --ext .js,.jsx src/ demo/src",
    "lint:fix": "eslint --fix --ext .js,.jsx src/ demo/src",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "gzip-size": "node -e \"process.stdout.write('JS gzip size: ')\" && gzip-size --raw dist/react-dropdown-aria.min.js",
    "analyze-bundle": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json demo/dist -s gzip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfangrad/react-dropdown-aria.git"
  },
  "keywords": [
    "react",
    "dropdown",
    "select",
    "aria",
    "react-dropdown",
    "react-dropdown-aria"
  ],
  "author": "Justin Fangrad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfangrad/react-dropdown-aria/issues"
  },
  "homepage": "https://github.com/jfangrad/react-dropdown-aria#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.14",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "concurrently": "^3.6.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "gzip-size-cli": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jest-emotion": "^9.2.7",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.9.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "rollup": "^0.62.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "uglify-es": "^3.3.10",
    "webpack": "^4.17.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "emotion": "^9.2.6",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "bundlesize": [
    {
      "path": "./dist/react-dropdown-aria.min.js"
    }
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/src/__tests__/__mocks__/styleMock.js"
    }
  }
}
