{
  "name": "react-dropdown-aria",
  "version": "1.3.1",
  "description": "Simple and accessible React dropdown component",
  "main": "dist/react-dropdown-aria.cjs.js",
  "module": "dist/react-dropdown-aria.esm.js",
  "typings": "./index.d.ts",
  "scripts": {
    "dev": "npm run demo:dev",
    "build": "npm run lib:prod && npm run demo:prod",
    "lib:dev": "preconstruct watch",
    "lib:prod": "preconstruct build",
    "demo:dev": "webpack-dev-server --mode development --open --hot",
    "demo:prod": "webpack --mode production",
    "deploy:demo": "npm run build && git push origin `git subtree split --prefix demo/dist master`:gh-pages --force",
    "lint": "eslint --ext .jsx src/ demo/src",
    "lint:fix": "eslint --fix --ext .js,.jsx src/ demo/src",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "gzip-size": "node -e \"process.stdout.write('JS gzip size: ')\" && gzip-size --raw dist/index.js",
    "analyze-bundle": "webpack --mode production --profile --json > stats.json && webpack-bundle-analyzer stats.json demo/dist -s gzip"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfangrad/react-dropdown-aria.git"
  },
  "keywords": [
    "react",
    "dropdown",
    "select",
    "aria",
    "react-dropdown",
    "react-dropdown-aria"
  ],
  "author": "Justin Fangrad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfangrad/react-dropdown-aria/issues"
  },
  "homepage": "https://github.com/jfangrad/react-dropdown-aria#readme",
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.14",
    "concurrently": "^3.6.1",
    "css-loader": "^0.28.11",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "gzip-size-cli": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.11",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.9.3",
    "preconstruct": "^0.0.82",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-hot-loader": "^4.3.4",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.17.0",
    "webpack-bundle-analyzer": ">=3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.4",
    "emotion": "^9.2.6",
    "prop-types": "^15.6.2"
  },
  "peerDependencies": {
    "react": "^16.4.0",
    "react-dom": "^16.4.0"
  },
  "bundlesize": [
    {
      "path": "./dist/index.js"
    }
  ],
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules"
    ],
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ],
    "snapshotSerializers": [
      "jest-emotion"
    ]
  },
  "preconstruct": {
    "source": "src/index.js"
  },
  "browser": {
    "./dist/react-dropdown-aria.cjs.js": "./dist/react-dropdown-aria.browser.cjs.js",
    "./dist/react-dropdown-aria.esm.js": "./dist/react-dropdown-aria.browser.esm.js"
  }
}
