{
  "name": "@react-dropdown-aria/monorepo",
  "private": true,
  "version": "1.0.0",
  "description": "Simple and accessible React dropdown component",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jfangrad/react-dropdown-aria.git"
  },
  "scripts": {
    "build": "yarn lib:prod && yarn demo:prod",
    "deploy": "yarn build && git subtree push --prefix demo/dist origin gh-pages",
    "demo:dev": "cd demo && yarn demo:dev",
    "demo:prod": "cd demo && yarn demo:prod",
    "lib:dev": "cd packages/react-dropdown-aria && yarn lib:dev",
    "lib:prod": "cd packages/react-dropdown-aria && yarn lib:prod",
    "lint": "cd packages/react-dropdown-aria && yarn lint",
    "test": "cd packages/react-dropdown-aria && yarn test",
    "test:update": "cd packages/react-dropdown-aria && yarn test:update"
  },
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-emotion": "^10.0.14",
    "css-loader": "^0.28.11",
    "emotion": "^9.2.6",
    "html-webpack-plugin": "^3.2.0",
    "jsx-loader": "^0.13.2",
    "node-sass": "^4.9.3",
    "prettier": "^1.18.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sass-loader": "^7.1.0",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "webpack": "^4.36.1",
    "webpack-bundle-analyzer": ">=3.3.2",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "keywords": [
    "react",
    "dropdown",
    "select",
    "aria",
    "react-dropdown",
    "react-dropdown-aria"
  ],
  "author": "Justin Fangrad",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jfangrad/react-dropdown-aria/issues"
  },
  "homepage": "https://jfangrad.github.io/react-dropdown-aria/",
  "workspaces": [
    "packages/*",
    "demo"
  ]
}
